ins],\n  props: {\n    columnas: {\n      type: String,\n      default: '',\n    },\n  },\n  methods: {\n    clickAnterior() {\n      const ids = this.elements.map(element => element.id)\n      const idxOfSelected = ids.indexOf(this.selected)\n      if (idxOfSelected > 0) {\n        this.selected = ids[idxOfSelected - 1]\n      }\n    },\n    clickSiguiente() {\n      const ids = this.elements.map(element => element.id)\n      const container = document.querySelector('#slyder-f-' + this.mainId)\n      const containerComputedStyles = window.getComputedStyle(container)\n      const containerWidth = parseFloat(containerComputedStyles.width)\n      const elm = document.querySelector(`#sl-${this.elements[0].id}`)\n      const elmComputedStyles = window.getComputedStyle(elm)\n      const elmWidth = parseFloat(elmComputedStyles.width)\n      const elmtsPerContainer = Math.round(containerWidth / elmWidth)\n      const newIds =\n        elmtsPerContainer > 1\n          ? ids.slice(0, ids.length - (elmtsPerContainer - 1))\n          : ids\n      const idxOfSelected = newIds.indexOf(this.selected)\n      if (idxOfSelected < newIds.length - 1) {\n        this.selected = newIds[idxOfSelected + 1]\n      }\n    },\n  },\n}\n</script>\n\n<style lang=\"sass\"></style>\n"],"mappings":"AA0BA,OAAAA,mBAAA;AACA,OAAAC,YAAA;AACA,OAAAC,gBAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,MAAA,GAAAL,mBAAA,EAAAC,YAAA;EACAK,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACA,MAAAC,GAAA,QAAAC,QAAA,CAAAC,GAAA,CAAAC,OAAA,IAAAA,OAAA,CAAAC,EAAA;MACA,MAAAC,aAAA,GAAAL,GAAA,CAAAM,OAAA,MAAAC,QAAA;MACA,IAAAF,aAAA;QACA,KAAAE,QAAA,GAAAP,GAAA,CAAAK,aAAA;MACA;IACA;IACAG,eAAA;MACA,MAAAR,GAAA,QAAAC,QAAA,CAAAC,GAAA,CAAAC,OAAA,IAAAA,OAAA,CAAAC,EAAA;MACA,MAAAK,SAAA,GAAAC,QAAA,CAAAC,aAAA,qBAAAC,MAAA;MACA,MAAAC,uBAAA,GAAAC,MAAA,CAAAC,gBAAA,CAAAN,SAAA;MACA,MAAAO,cAAA,GAAAC,UAAA,CAAAJ,uBAAA,CAAAK,KAAA;MACA,MAAAC,GAAA,GAAAT,QAAA,CAAAC,aAAA,aAAAV,QAAA,IAAAG,EAAA;MACA,MAAAgB,iBAAA,GAAAN,